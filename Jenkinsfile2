@Library('shared-library@master') _ //master or whatever branch

pipeline{

      agent any
      tools {
        maven '3.6.3' 
    }

        
        stages{

              stage('maven build'){
                  steps{
	                   script{
                              sh "mvn clean install"
                      	  }
               	     }  
                 }	
                 
                 stage ('Check logs') {
                    steps {
                        filterLogs ('WARNING', 1)
                    }
                }
		
           }	       	     	         
}
	       	     	         

