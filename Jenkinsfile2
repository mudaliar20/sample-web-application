

//@Library('shared-library@master') _ //master or whatever branch

pipeline{

      agent any

        
        stages{

              stage('maven build'){
                  steps{
                      
		    	        def mavenHome =  tool name: "3.6.3", type: "maven"
                                def mavenCMD = "${mavenHome}/bin/mvn"
	                   script{
                              sh "${mavenCMD} clean package"            
                      	  }
               	     }  
                 }	
                 
                 stage ('Check logs') {
                    steps {
                        filterLogs ('WARNING', 1)
                    }
                }
		
           }	       	     	         
}
	       	     	         

